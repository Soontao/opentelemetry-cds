{
  "name": "opentelemetry-cds",
  "description": "opentelemetry support for CDS nodejs runtime",
  "version": "0.1.1",
  "main": "lib/index.js",
  "bin": {
    "otlp-cds": "./bin/otlp-cds.js"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "pretest": "bash install_no_trace_deps.sh",
    "prelint": "bash install_no_trace_deps.sh",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.1.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.31.0",
    "@opentelemetry/instrumentation": "0.31.0",
    "@opentelemetry/instrumentation-dns": "^0.29.0",
    "@opentelemetry/instrumentation-express": "^0.30.0",
    "@opentelemetry/instrumentation-generic-pool": "^0.29.0",
    "@opentelemetry/instrumentation-http": "0.31.0",
    "@opentelemetry/instrumentation-mysql2": "^0.31.0",
    "@opentelemetry/instrumentation-net": "^0.29.0",
    "@opentelemetry/resources": "^1.3.1",
    "@opentelemetry/sdk-trace-base": "^1.3.1",
    "@opentelemetry/sdk-trace-node": "^1.3.1",
    "@opentelemetry/semantic-conventions": "^1.3.1",
    "cds-internal-tool": "^1.2.0"
  },
  "devDependencies": {
    "@types/jest": "28.1.7",
    "@types/node": "16.11.48",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "eslint": "8.22.0",
    "eslint-plugin-jest": "26.8.3",
    "jest": "28.1.3",
    "jest-junit": "14.0.0",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/opentelemetry-cds"
  },
  "keywords": [
    "cds",
    "CAP",
    "opentelemetry",
    "trace"
  ],
  "license": "Apache-2.0"
}
