{
  "name": "opentelemetry-cds",
  "description": "OpenTelemetry support for CDS nodejs runtime",
  "version": "0.0.1",
  "main": "lib/index.js",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "test": "jest --detectOpenHandles --passWithNoTests",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.1.0",
    "@opentelemetry/exporter-jaeger": "^1.3.1",
    "@opentelemetry/instrumentation": "0.29.2",
    "@opentelemetry/instrumentation-express": "^0.30.0",
    "@opentelemetry/instrumentation-http": "0.29.2",
    "@opentelemetry/resources": "^1.3.1",
    "@opentelemetry/sdk-trace-base": "^1.3.1",
    "@opentelemetry/sdk-trace-node": "^1.3.1",
    "@opentelemetry/semantic-conventions": "^1.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.41",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.18.0",
    "eslint-plugin-jest": "^26.5.3",
    "jest": "^27.5.1",
    "jest-junit": "^13.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "license": "Apache-2.0"
}
