{
  "name": "opentelemetry-cds",
  "description": "opentelemetry support for CDS nodejs runtime",
  "version": "0.1.4",
  "main": "lib/index.js",
  "bin": {
    "otlp-cds-serve": "./bin/otlp-cds-serve.js"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "scripts": {
    "pretest": "node install_optional_deps.js",
    "prelint": "node install_optional_deps.js",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.51.0",
    "@opentelemetry/instrumentation": "0.51.1",
    "@opentelemetry/instrumentation-dns": "^0.36.0",
    "@opentelemetry/instrumentation-express": "^0.39.0",
    "@opentelemetry/instrumentation-generic-pool": "^0.36.0",
    "@opentelemetry/instrumentation-http": "0.51.1",
    "@opentelemetry/instrumentation-mysql2": "^0.38.0",
    "@opentelemetry/instrumentation-net": "^0.36.0",
    "@opentelemetry/resources": "^1.3.1",
    "@opentelemetry/sdk-trace-base": "^1.3.1",
    "@opentelemetry/sdk-trace-node": "^1.19.0",
    "@opentelemetry/semantic-conventions": "^1.3.1",
    "cds-internal-tool": "^1.7.8"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/node": "20.14.2",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "eslint": "9.4.0",
    "eslint-plugin-jest": "28.5.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "ts-jest": "29.1.4",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/opentelemetry-cds"
  },
  "keywords": [
    "cds",
    "CAP",
    "opentelemetry",
    "trace"
  ],
  "license": "Apache-2.0"
}
